public class Exercicio1_4{
	public static void main(String [] args){
		float a=45;
		float b=20;
		float c=3;
		float soma=a+b+c;
		float multiplicacao=a*b*c;
 		float subtracao=a-b-c ;
		float divisao=a/b/c;
		System.out.println("O valor da soma: " + soma);
		System.out.println("O valor da multiplicacao: " +  multiplicacao);
		System.out.println("O valor da divisao: " +  divisao);
		System.out.println("O valor da subtra√ßao: " + subtracao);
	}
}
